local toggle = false
script.Parent.MouseButton1Click:connect(function()
	if(not toggle) then
		script.Parent.Text = ("âœ“")
		toggle = true
		while toggle do
			wait()
			if workspace.Bots:FindFirstChild("Mouselocking AI") then
				local Bot = workspace.Bots["Mouselocking AI"]
				Bot["Sword bot"].Humanoid.Health = 0
				Bot.Name = "Killed"
			else
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "WSBG Hack";
					Text = "All bots killed, or none found.";
					Duration = 5;
					Button1 = "Dismiss";
				})
				script.Parent.Text = ""
				toggle = false
				
			end
		end
		
		else if (toggle) then
				script.Parent.Text = ""
				toggle = false
			end
	end
end)
