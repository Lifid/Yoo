local toggle = false
script.Parent.MouseButton1Click:connect(function()
	if(not toggle) then
		script.Parent.Text = ("âœ“")
		toggle = true
		while toggle do
			local BossLasers = workspace.BossLasers
			local LaserFolder = workspace.LaserFolder
			local Spikes = workspace.Cages
			while true do
				wait()
				for i,v in pairs(BossLasers:GetChildren()) do
					v:Destroy()

					for i,v in pairs(LaserFolder:GetChildren()) do
						v:Destroy()

						for i,v in pairs(Spikes:GetChildren()) do
							v:Destroy()
						end
					end
				end
			end
		end

	else if (toggle) then
			script.Parent.Text = ""
			toggle = false
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "WSBG Hack";
				Text = "Autokill Golem - Off";
				Duration = 5;
				Button1 = "Dismiss";
			})

		end
	end
end)
